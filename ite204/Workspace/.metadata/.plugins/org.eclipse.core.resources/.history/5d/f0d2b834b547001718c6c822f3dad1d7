import javax.swing.JOptionPane;

public class HW3 {

	public static void main(String[] args) {
		int[] nums = new int[3];
		
		for(int i = 0; i < nums.length; i++){
			nums[i] = Integer.parseInt(JOptionPane.showInputDialog("Enter num" + (i + 1)));
		}
		
		int max = nums[0];
		int min = nums[0];
		
		for(int i = 1; i < nums.length; i++){
			if(nums[i] > max){
				
				max = nums[i];
			
			}else if(nums[i] < min){
				
				min = nums[i];
				
			}
		}
		
		
		
		
		
		
		
	}

}
