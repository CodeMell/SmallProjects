package day05;

public class TestCircle {

	public static void main(String[] args) {
		
		Circle mycircle = new Circle(9.8, "orange");
		Circle yourcircle = new Circle(6.78, "purple");
		
		System.out.println("Area of mycircle is " + mycircle.findArea() + "\n");
		
		System.out.println("Color of yourcircle is " + yourcircle.color + "\n");

	}

}
